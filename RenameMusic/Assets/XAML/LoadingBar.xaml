<Window x:Class="RenameMusic.LoadingBar"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RenameMusic"
        xmlns:p = "clr-namespace:RenameMusic.Lang"
        mc:Ignorable="d"
        Title="{x:Static p:Strings.LOADING}" Height="60" Width="360">
    <Grid>
        <ProgressBar x:Name="progressBar" HorizontalAlignment="Center" VerticalAlignment="Center" Height="20" Width="300" ValueChanged="ProgressBar_ValueChanged"/>
    </Grid>
</Window>
